pragma solidity >=0.8.2 <0.9.0;

contract ValorReferencia {

    uint public valor;
    string public name;

    function sertValor(uint _valor) public {
        valor = _valor;

    }

    function getName(string memory _name) public {
        name = _name;
    }
}
